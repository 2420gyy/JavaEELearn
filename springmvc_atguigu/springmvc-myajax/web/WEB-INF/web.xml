<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>BmiServlet</servlet-name>
        <servlet-class>com.bjpower.controller.BmiServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BmiServlet</servlet-name>
        <url-pattern>/bmiServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BmiPrintServlet</servlet-name>
        <servlet-class>com.bjpower.controller.BmiPrintServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BmiPrintServlet</servlet-name>
        <url-pattern>/bmiPrintServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BmiAjaxServlet</servlet-name>
        <servlet-class>com.bjpower.controller.BmiAjaxServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BmiAjaxServlet</servlet-name>
        <url-pattern>/bmiAjaxServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SearchAjaxServlet</servlet-name>
        <servlet-class>com.bjpower.controller.SearchAjaxServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchAjaxServlet</servlet-name>
        <url-pattern>/searchAjaxServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>QueryJsonServlet</servlet-name>
        <servlet-class>com.bjpower.controller.QueryJsonServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>QueryJsonServlet</servlet-name>
        <url-pattern>/queryJson</url-pattern>
    </servlet-mapping>
</web-app>